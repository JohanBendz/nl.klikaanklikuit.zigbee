  "drivers": [
    {
      "id": "smart_dimmable_led_bulb_E27",
      "name": {
        "en": "Smart Dimmable LED Bulb",
        "nl": "Slimme Dimbare LED Lamp"
      },
      "class": "light",
      "capabilities": [
        "onoff",
        "dim"
      ],
      "zigbee": {
        "manufacturerName": "Trust International B.V.",
        "productId": "ZLL-DimmableLigh",
        "deviceId": 256,
        "profileId": 49246,
        "learnmode": {
          "image": "/drivers/smart_dimmable_led_bulb_E27/assets/icon.svg",
          "instruction": {
            "en": "Toggle the bulb to start pairing.\n\nIf pairing does not automatically start within 20 seconds, try resetting the bulb by toggling it 5 times quickly. It will flash twice to indicate it has been reset.",
            "nl": "Zet de lamp uit en weer aan.\n\nWordt de lamp niet automatisch toegevoegd binnen 20 seconden, probeer het dan te resetten door 5 keer de lamp aan en uit te zetten. De lamp zal twee keer knipperen om aan te geven dat hij is gereset."
          }
        }
      },
      "images": {
        "large": "/drivers/smart_dimmable_led_bulb_E27/assets/images/large.png",
        "small": "/drivers/smart_dimmable_led_bulb_E27/assets/images/small.png"
      },
      "settings": [
        {
          "id": "transtime",
          "type": "number",
          "label": {
            "nl": "Transitie tijd in seconden",
            "en": "Transition time in seconds"
          },
          "value": 1,
          "attr": {
            "min": 0,
            "max": 20
          }
        }
      ]
    },
